<?xml version="1.0"?>

<robot name="abb_1200" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find abb_arm)/description/urdf/end_effector_contact.xacro" />
    <xacro:include filename="$(find abb_arm)/description/urdf/transmission.xacro" />
    <xacro:include filename="$(find abb_arm)/description/urdf/gazebo_config.xacro" />
    <xacro:include filename="$(find abb_arm)/description/urdf/gazebo_ros2_control.xacro" />

    <xacro:macro name="abb_1200">
        <!-- World fixed joint-->
        <link name="world" />
        <joint name="world_fixed" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <parent link="world"/>
            <child link="base_0"/>
        </joint>

        <!-- Base -->
        <link name="base_0">
            <inertial>
                <mass value="13.7742"/>
                <origin xyz="-0.028986 0.000596 0.11273"/>
                <inertia ixx="0.101998" ixy="0.000495482" ixz="0.000311158" iyy="0.13969" iyz="-0.000245375" izz="0.130433"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/base_link.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/base_link.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 1 -->
		<joint name="joint_1" type="revolute">
			<parent link="base_0" />
			<child link="link_1" />
			<origin rpy="0 0 0" xyz="0 0 0.3991" />
			<axis xyz="0 0 1" />
			<limit effort="1000" lower="-2.967" upper="2.967" velocity="5.027"/>
			<dynamics damping="50.0" friction="1.0" />
		</joint>

        <!-- Link 1 -->
        <link name="link_1">
            <inertial>
                <mass value="11.8419"/>
                <origin xyz="0.000877 -0.000631 -0.062883"/>
                <inertia ixx="0.11194" ixy="-4.54988e-05" ixz="0.000280961" iyy="0.0915159" iyz="-0.000109905" izz="0.0876456"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_1.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_1.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 2 -->
		<joint name="joint_2" type="revolute">
			<parent link="link_1" />
			<child link="link_2" />
			<origin rpy="0 0 0" xyz="0 0 0" />
			<axis xyz="0 1 0" />
			<limit effort="1000" lower="-1.745" upper="2.269" velocity="4.189"/>
			<dynamics damping="50.0" friction="1.0" />
		</joint>

        <!-- Link 2 -->
        <link name="link_2">
            <inertial>
                <mass value="17.5394"/>
                <origin xyz="-0.000928 -0.000497 0.250051"/>
                <inertia ixx="0.493558" ixy="1.32136e-05" ixz="0.000209024" iyy="0.462939" iyz="-0.00179901" izz="0.0894214"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_2.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_2.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 3 -->
		<joint name="joint_3" type="revolute">
			<parent link="link_2" />
			<child link="link_3" />
			<origin rpy="0 0 0" xyz="0 0 0.448" />
			<axis xyz="0 1 0" />
			<limit effort="1000" lower="-3.491" upper="1.222" velocity="5.236"/>
			<dynamics damping="10.0" friction="1.0" />
		</joint>

        <!-- Link 3 -->
        <link name="link_3">
            <inertial>
                <mass value="7.46365"/>
                <origin xyz="0.099588 0.001143 0.032333"/>
                <inertia ixx="0.0252424" ixy="0.000142737" ixz="-0.00565542" iyy="0.0906438" iyz="0.000142213" izz="0.0825079"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_3.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_3.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 4 -->
		<joint name="joint_4" type="revolute">
			<parent link="link_3" />
			<child link="link_4" />
			<origin rpy="0 0 0" xyz="0 0 0.042" />
			<axis xyz="1 0 0" />
			<limit effort="1000" lower="-4.712" upper="4.712" velocity="6.981"/>
			<dynamics damping="5.0" friction="1.0" />
		</joint>

        <!-- Link 4 -->
        <link name="link_4">
            <inertial>
                <mass value="2.74436"/>
                <origin xyz="0.381678 0.001261 0.005168"/>
                <inertia ixx="0.00573099" ixy="-0.000131119" ixz="0.000380232" iyy="0.0118352" iyz="-2.26565e-05" izz="0.0114428"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_4.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_4.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 5 -->
		<joint name="joint_5" type="revolute">
			<parent link="link_4" />
			<child link="link_5" />
			<origin rpy="0 0 0" xyz="0.451 0 0" />
			<axis xyz="0 1 0" />
			<limit effort="1000" lower="-2.269" upper="2.269" velocity="7.069"/>
			<dynamics damping="2.0" friction="1.0" />
		</joint>

        <!-- Link 5 -->
        <link name="link_5">
            <inertial>
                <mass value="0.62953"/>
                <origin xyz="0.011197 -0.001056 0.000109"/>
                <inertia ixx="0.000502815" ixy="-1.03173e-05" ixz="-8.74347e-08" iyy="0.00108856" iyz="2.07657e-07" izz="0.000918873"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_5.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_5.dae"/>
                </geometry>
            </visual>
        </link>

        <!-- joint 6 -->
		<joint name="joint_6" type="revolute">
			<parent link="link_5" />
			<child link="link_6" />
			<origin rpy="0 0 0" xyz="0.082 0 0" />
			<axis xyz="1 0 0" />
			<limit effort="1000" lower="-6.283" upper="6.283" velocity="10.472"/>
			<dynamics damping="1.0" friction="1.0" />
		</joint>

        <!-- Link 6 -->
        <link name="link_6">
            <inertial>
                <mass value="0.137"/>
                <origin xyz="-0.00706 -0.00017 -1.32E-06"/>
                <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
            </inertial>
            <collision name="collision">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/collision/link_6.stl"/>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="file:///$(find abb_arm)/description/meshes/visual/link_6.dae"/>
                </geometry>
            </visual>
        </link>

    </xacro:macro>

    <!-- Invoke the macro -->
    <xacro:abb_1200/>
    <xacro:endeffector_contact_sensor />
    <xacro:dsr_transmission />
    <xacro:dsr_config_coeff/>
    <xacro:config_ctr_gazebo/>

</robot>
